{
    "manifest_version": 3,
    "name": "Paragraph Reader",
    "version": "1.0.0",
    "description": "Read paragraphs from a webpage",
    "permissions": ["activeTab"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  